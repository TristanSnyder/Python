    "from tensorflow.keras.callbacks import ReduceLROnPlateau, EarlyStopping\n",
    "from tensorflow.keras.optimizers import AdamW\n",
    "\n",
    "model.compile(\n",
    "    loss='binary_crossentropy',\n",
    "    optimizer=AdamW(\n",
    "        learning_rate=5e-5,   # same LR as before\n",
    "        weight_decay=1e-6     # add a small weight decay (tune as needed)\n",
    "    ),\n",
    "    metrics=['AUC']\n",
    ")\n",
    "\n",
    "reduce_lr = ReduceLROnPlateau(\n",
    "    monitor='val_loss',\n",
    "    factor=0.2,            # shrink LR to 20% when stalled\n",
    "    patience=5,\n",
    "    min_lr=1e-6,           # floor so it doesn’t vanish\n",
    "    verbose=1\n",
    ")\n",
    "early_stop = EarlyStopping(\n",
    "    monitor='val_loss',\n",
    "    patience=9,            # give the new LR a chance\n",
    "    restore_best_weights=True,\n",
    "    verbose=1\n",
    ")\n",
    "\n",
    "history = model.fit(\n",
    "    sequences_matrix, Y_train,\n",
    "    validation_split=0.2,\n",
    "    epochs=50,\n",
    "    batch_size=32,\n",
    "    callbacks=[reduce_lr, early_stop],\n",
    "    class_weight={0:1.0, 1:2.5},   # up class‑1 weight to 2.5\n",
    "    shuffle=True,\n",
    "    verbose=1\n",
    ")"
   ]
